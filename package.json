{
  "name": "cloudlint",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b tsconfig.app.json tsconfig.node.json && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit",
    "type-check:test": "tsc --noEmit --project tsconfig.test.json",
    "yaml:validate": "tsx src/backend/yaml/cli.ts validate",
    "yaml:fix": "tsx src/backend/yaml/cli.ts fix",
    "yaml:suggest": "tsx src/backend/yaml/cli.ts suggest",
    "schemas:fetch": "tsx src/backend/yaml/schemaFetch.ts fetch",
    "start:server": "tsx src/server.ts",
    "e2e": "playwright install && playwright test",
"start:e2e": "npm run build && tsx src/server.ts",
    "start:prod:pwsh": "npm run build && pwsh -NoLogo -NoProfile -Command \"$env:SERVE_STATIC='1'; $env:PORT='8787'; npm run start:server\""
  },
  "dependencies": {
    "@lottiefiles/dotlottie-react": "^0.17.2",
    "@radix-ui/react-dialog": "^1.1.3",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/vite": "^4.1.13",
    "ajv": "^8.17.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "diff": "^5.2.0",
    "express": "^4.21.1",
    "express-rate-limit": "^7.4.0",
    "lucide-react": "^0.544.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.0.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.13",
    "yaml": "^2.6.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@playwright/test": "^1.48.2",
    "@stoplight/spectral-cli": "^6.14.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/cors": "^2.8.19",
    "@types/diff": "^7.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/supertest": "^6.0.3",
    "@vitejs/plugin-react": "^5.0.3",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "happy-dom": "^18.0.1",
    "jsdom": "^27.0.0",
    "msw": "^2.11.3",
    "prettier": "^3.3.3",
    "shadcn": "^3.3.1",
    "supertest": "^7.0.0",
    "tsx": "^4.19.1",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.7",
    "vitest": "^3.2.4"
  }
}
